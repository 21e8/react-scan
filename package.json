{
  "name": "react-scan",
  "version": "0.0.0",
  "scripts": {
    "build": "pnpm build:dev && pnpm build:prod",
    "build:dev": "tsup src/development.ts --dts --env.NODE_ENV development --format esm",
    "build:prod": "tsup src/production.ts --env.NODE_ENV production",
    "publint": "publint"
  },
  "dependencies": {
    "react-reconciler": "^0.29.2",
    "tsup": "^8.2.4"
  },
  "devDependencies": {
    "@types/react-reconciler": "^0.28.8",
    "publint": "^0.2.10"
  }
}
